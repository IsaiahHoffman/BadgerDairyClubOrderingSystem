<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Order Processing</title>
    <link rel="stylesheet" href="orderProcessing.css">
</head>

<body>
    <div id="options">
        <div>New Order</div>
        Ticket Number: 
        <input type="number" id="ticket" name="ticket">
        <div id="order">Search</div>
    </div>
    <div class=items hidden="True">
        <div class="item">
            <div class="increment fl">+</div>
            <div class="increment fr">-</div>
            <div>American</div>
            <div class="count">5</div>
        </div>
        <div class="item">
            <div class="increment fl">+</div>
            <div class="increment fr">-</div>
            <div>Swiss</div>
            <div class="count">5</div>
        </div>
        <div class="item">
            <div class="increment fl">+</div>
            <div class="increment fr">-</div>
            <div>Specialty</div>
            <div class="count">5</div>
        </div>
        <div class="item">
            <div class="increment fl">+</div>
            <div class="increment fr">-</div>
            <div>Ham & Cheese</div>
            <div class="count">5</div>
        </div>
    </div>


</body>
<script src="orderProcessing.js"></script>

</html>